{
    "name": "awes-io/form-builder",
    "description": "A component that allows creating forms with two-way binding from data object with default HTML fields and custom, like multi-block or AJAX-select.",
    "type": "library",
    "license": "MIT",
	"authors": [
		{
			"name": "Awescode GmbH",
			"email": "info@awescode.de",
			"homepage": "https://www.awescode.de",
			"role": "Owner"
		},
        {
            "name": "theAlex",
            "email": "info@thealex.ru",
            "homepage": "https://thealex.ru"
        }
	],
	"support": {
		"email": "contact@awes.io"
	},
	"homepage": "https://github.com/awes-io/form-builder",
	"keywords": ["Laravel", "Form Builder", "ui forms", "Awes.io"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
		"phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AwesIO\\FormBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AwesIO\\FormBuilder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwesIO\\FormBuilder\\FormBuilderServiceProvider"
            ]
        }
    }
}
